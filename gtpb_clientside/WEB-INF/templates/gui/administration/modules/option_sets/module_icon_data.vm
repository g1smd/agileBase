## TODO: for some reason, script isn't called when in external tabs.js
<script type="text/javascript">
  $("td[data-iconname='$currentIconPath']").attr("id","icon_selected");
  fModuleIconPicker();

  function fModuleIconPicker() {
	$("table.iconPickerTable td").click(function() {
	  var selected = $(this);
		$("table.iconPickerTable td").removeAttr("id");
		var iconName = $(this).attr("data-iconname");
		jQuery.post("AppController.servlet", {
			"return": "blank",
			update_module: true,
			iconpath: iconName
		}, function() {
			selected.attr("id", "icon_selected");
		});
	});
}
</script>

