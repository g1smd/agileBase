<!doctype html>
<html>
<head>
  <script language="JavaScript" src="resources/jquery.js"></script>
  <script language="JavaScript" src="resources/preview/argument.js"></script>
  <link rel="stylesheet" type="text/css" href="resources/preview/argument.css" />
  <link rel="stylesheet" type="text/css" href="resources/pane1_amalgam.css" />
</head>
<body>
  <div id="sets">
      #set($module = $sessionData.getModule())
	  <h1><i class="fonticon icon-large $module.getIconPath()" ></i> $module: choose set</h1><p>
	  #foreach($report in $view.getAllViewableReports())
	    #if($module.equals($report.getModule()))
	      ## See if the report contains any images
	      #foreach($reportField in $report.getReportFields())
	        #if($reportField.getBaseField().getFieldCategory() == "FILE")
	          <button data-internaltablename="$report.getParentTable().getInternalTableName()" value="$report.getInternalReportName()">$report</button><p>
	          #break
	        #end
	      #end
	    #end
	  #end
  </div>
</body>
</html>
